<Defs>

  <GeneDef>
    <defName>DDJY_MilkProduction</defName>
    <label>产奶</label>
    <description>从奶牛身上提取的基因，经过基因改写技术，已经可以安全的与人类基因融合，可以使人源源不断的分泌乳汁。</description>
    <iconPath>Genes/Milk</iconPath>
    <geneClass>DDJY.Gene_MilkProduction</geneClass>
    <displayCategory>DDJY_GatherHumanCategory</displayCategory>
    <displayOrderInCategory>400</displayOrderInCategory>
    <customEffectDescriptions>
      <li>可以产奶</li>
    </customEffectDescriptions>
    <exclusionTags>
      <li>DDJY_MilkProduction</li>
    </exclusionTags>
    <modExtensions>
      <li Class="DDJY.DefModExtension_Gene">
        <interval>1</interval>
        <amount>14</amount>
        <produce>Milk</produce>
        <femaleOnly>true</femaleOnly>
        <fullChargeAmount>0.5</fullChargeAmount>
      </li>
    </modExtensions>
    <minAgeActive>18</minAgeActive>
    <biostatCpx>1</biostatCpx>
    <biostatMet>-1</biostatMet>
  </GeneDef>


